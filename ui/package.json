    "@turf/distance": "^6.0.1",
    "aws-amplify": "^3.0.8",
    "bootstrap": "^4.4.1",
    "changelog-parser": "2.8.0",
    "chokidar": "^3.3.1",
    "connected-react-router": "^6.8.0",
    "cross-env": "^7.0.2",
    "downshift": "5.2.0",
    "formik": "^2.1.4",
    "history": "^4.10.1",
    "lodash": "4.17.20",
    "mapbox-gl": "^1.10.1",
    "moment": "^2.26.0",
    "node-less-chokidar": "^0.4.1",
    "react": "^16.13.1",
    "react-autocomplete": "^1.8.1",
    "react-datepicker": "^2.14.1",
    "react-dom": "^16.13.1",
    "react-fontawesome": "^1.7.1",
    "react-json-view": "^1.19.1",
    "react-mapbox-gl": "^4.8.6",
    "react-redux": "^7.2.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "react-swipeable": "^5.5.1",
    "reactstrap": "^8.4.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "run-p": "0.0.0",
    "webfontloader": "^1.6.28"
  },
  "scripts": {
    "start": "yarn run build-css && run-p -ncr watch-css start-js",
    "build": "cross-env GENERATE_SOURCEMAP=false run-s -n build-css build-js",
    "test": "run-s -n build-css test-js",
    "start-js": "PORT=3003 react-app-rewired start",
    "build-js": "CI=false react-app-rewired build",
    "test-js": "react-app-rewired test --env=jsdom",
    "build-css": "node-less-chokidar src",
    "watch-css": "node-less-chokidar src --watch",
    "eject": "react-app-rewired eject",
    "lint": "eslint ./src",
    "format": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|md|vue)\"",
    "serve": "ws --port 3000 -d build --log.format dev --spa index.html"
  },
  "prettier": {
    "printWidth": 120,
    "bracketSpacing": true,
    "trailingComma": "es5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "less": "^3.10.3",
    "local-web-server": "4.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react-app-rewired": "^2.1.5"
  }
}
